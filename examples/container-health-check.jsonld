{
  "@context": "https://schema.org",
  "@type": "CheckAction",
  "name": "Nginx Container Health Check",
  "description": "Monitors the Nginx web server container every 2 minutes to ensure it's serving content correctly. Checks both HTTP status and response time.",
  "agent": "localhost-docker",
  "actionStatus": "PotentialActionStatus",
  "enabled": true,
  "startTime": null,
  "endTime": null,
  "schedule": {
    "@type": "Schedule",
    "repeatFrequency": "PT2M",
    "scheduleTimezone": "UTC",
    "byDay": ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"]
  },
  "instrument": {
    "url": "http://localhost:8081",
    "method": "GET",
    "expectedStatusCode": 200,
    "timeout": 10
  },
  "object": {
    "@type": "SoftwareApplication",
    "name": "nginx-web-server",
    "identifier": "nginx-multihost-nginx-1"
  }
}
