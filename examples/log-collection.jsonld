{
  "@context": "https://schema.org",
  "@type": "TransferAction",
  "name": "Hourly Application Log Collection",
  "description": "Collect last 1000 lines of application logs every hour for analysis and archival.",
  "agent": "localhost-docker",
  "actionStatus": "PotentialActionStatus",
  "enabled": true,
  "startTime": null,
  "endTime": null,
  "schedule": {
    "@type": "Schedule",
    "repeatFrequency": "PT1H",
    "scheduleTimezone": "UTC",
    "byDay": ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"]
  },
  "instrument": {
    "action": "collect-logs",
    "containerId": "nginx-multihost-nginx-1",
    "lines": 1000,
    "since": "1h",
    "destination": "file:///home/opunix/graphium/collected/"
  },
  "object": {
    "@type": "SoftwareApplication",
    "name": "nginx-multihost",
    "identifier": "nginx-multihost-nginx-1"
  }
}
